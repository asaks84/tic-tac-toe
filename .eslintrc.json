{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns":["**/dist/*.js"],
    "rules": {
        "no-param-reassign": "off",
        "no-use-before-define": "off"
    }
}
